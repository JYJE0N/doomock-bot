# 개발용 설정
NODE_ENV=development
BOT_TOKEN=hello_world_bot_token
BOT_USERNAME=doomock_todoBot
PORT=3000

# 로깅
LOG_LEVEL=info
CONSOLE_LOGGING=true

# 작업 시간
WORK_START=09:00
WORK_END=18:00
LUNCH_START=12:00
LUNCH_END=13:00

# 기능 토글
ENABLE_TODO_MODULE=true
ENABLE_LEAVE_MODULE=true
ENABLE_WEATHER_MODULE=true
ENABLE_FORTUNE_MODULE=true
ENABLE_TIMER_MODULE=true